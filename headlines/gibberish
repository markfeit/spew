%VOWEL
a
e
i
o
u

%ENDLETTER
(3)\LCCONSONANT
\VOWEL

%PUNCT
(20)
:
;
,


%ENDPUNCT
(3).
!
?

%WORDINNARDS
\LCCONSONANT\VOWEL
(1)\LCCONSONANT\VOWEL\WORDINNARDS

%WORD
\LCCONSONANT\VOWEL\WORDINNARDS\ENDLETTER

%WORDS
\WORD
(10)\WORD\PUNCT \WORDS

%SENTENCES
\UCCONSONANT\VOWEL\WORD \WORDS\ENDPUNCT\!

%MAIN
In the immortal words of \PERSON:\!\!\SENTENCES
